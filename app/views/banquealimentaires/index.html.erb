<p id="notice"><%= notice %></p>

<h1>Banquealimentaires</h1>

<table>
  <thead>
    <tr>
      <th>Statut</th>
      <th>Situation</th>
      <th>Actu</th>
      <th>Nombre de personnes</th>
      <th>Nombre handicapes</th>
      <th>Nombre vieux</th>
      <th>Nombre jeune mere</th>
      <th>Nombre jeune</th>
      <th>Nombre nourisson</th>
      <th>Chef</th>
      <th>Distribue</th>
      <th>Compte</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @banquealimentaires.each do |banquealimentaire| %>
      <tr>
        <td><%= banquealimentaire.statut_id %></td>
        <td><%= banquealimentaire.situation_id %></td>
        <td><%= banquealimentaire.actu_id %></td>
        <td><%= banquealimentaire.nombre_de_personnes %></td>
        <td><%= banquealimentaire.nombre_handicapes %></td>
        <td><%= banquealimentaire.nombre_vieux %></td>
        <td><%= banquealimentaire.nombre_jeune_mere %></td>
        <td><%= banquealimentaire.nombre_jeune %></td>
        <td><%= banquealimentaire.nombre_nourisson %></td>
        <td><%= banquealimentaire.chef_id %></td>
        <td><%= banquealimentaire.distribue_id %></td>
        <td><%= banquealimentaire.compte_id %></td>
        <td><%= link_to 'Show', banquealimentaire %></td>
        <td><%= link_to 'Edit', edit_banquealimentaire_path(banquealimentaire) %></td>
        <td><%= link_to 'Destroy', banquealimentaire, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Banquealimentaire', new_banquealimentaire_path %>
