<p id="notice"><%= notice %></p>

<h1>Piece Locataires</h1>

<table>
  <thead>
    <tr>
      <th>Nom</th>
      <th>Description</th>
      <th>Slug</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @piece_locataires.each do |piece_locataire| %>
      <tr>
        <td><%= piece_locataire.nom %></td>
        <td><%= piece_locataire.description %></td>
        <td><%= piece_locataire.slug %></td>
        <td><%= link_to 'Show', piece_locataire %></td>
        <td><%= link_to 'Edit', edit_piece_locataire_path(piece_locataire) %></td>
        <td><%= link_to 'Destroy', piece_locataire, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Piece Locataire', new_piece_locataire_path %>
